int ledPin = 13;

void setup (){
  pinMode(ledPin, OUTPUT);
}

void loop(){
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(100);
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(100);
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(750); //end//
  digitalWrite(ledPin, 1);
  delay(500);
  digitalWrite(ledPin, 0);
  delay(500);
  digitalWrite(ledPin, 1);
  delay(500);
  digitalWrite(ledPin, 0);
  delay(500);
  digitalWrite(ledPin, 1);
  delay(500);
  digitalWrite(ledPin, 0);
  delay(750); //end//
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(100);
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(100);
  digitalWrite(ledPin, 1);
  delay(100);
  digitalWrite(ledPin, 0);
  delay(750); //end//
}
